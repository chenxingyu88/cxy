.lists{
  width: 100%;
  margin-top: 10px;
}
.roomType {
  width: 50%;
  height: 95px;
  float: left;
  box-sizing: border-box;
  padding: 0 15px;
}
.roomType image {
  width: 100%;
  height: 70px;
}
.roomType text {
  display: block;
  width: 100%;
  font-size: 12px;
  padding-bottom: 5px;
  text-align: center;
}
.btn{
  width: 130px;
  height: 30px;
  line-height: 30px;
  background: blue;
  color:#fff;
  font-size: 14px;
}
